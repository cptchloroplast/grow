{
	"version": "2.0.0",
	"tasks": [
    {
      "label": "Server:Develop",
      "dependsOn": [
        "Redis:Start",
        "SQL:Start",
        "SQL:Migrations:Upgrade"
      ],
      "dependsOrder": "sequence"
    },
    {
      "label": "SQL:Start",
      "command": "make",
      "args": ["sql.start"]
    },
    {
      "label": "Redis:Start",
      "command": "make",
      "args": ["redis.start"]
    },
    {
      "label": "SQL:Migrations:Upgrade",
      "command": ".venv/bin/alembic",
      "args": ["upgrade", "head"],
      "options": {
        "cwd": "${workspaceFolder}/server"
      }
    },
  ]
}